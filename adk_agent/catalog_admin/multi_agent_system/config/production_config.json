{
  "orchestrator": {
    "max_parallel_workers": 5,
    "system_user_id": "catalog_orchestrator_prod",
    "log_retention_days": 30
  },
  "agents": {
    "triage": {
      "enabled": true,
      "batch_size": 20,
      "max_retries": 3,
      "timeout_seconds": 30
    },
    "enrichment": {
      "enabled": true,
      "batch_size": 10,
      "max_exercises_per_run": 100,
      "parallel": true
    },
    "janitor": {
      "enabled": true,
      "dry_run_first": true,
      "max_families_per_run": 50,
      "confidence_threshold": 0.8
    },
    "scout": {
      "enabled": false,
      "min_search_frequency": 5,
      "lookback_hours": 24
    },
    "auditor": {
      "enabled": true,
      "dry_run": true,
      "report_to_email": null
    },
    "approval": {
      "enabled": true,
      "auto_approve_threshold": {
        "has_family": true,
        "has_variant": true,
        "has_aliases": true,
        "min_alias_count": 1
      }
    }
  },
  "schedules": {
    "triage": {
      "frequency": "every_15_minutes",
      "quiet_hours": {
        "enabled": false,
        "start": "22:00",
        "end": "06:00"
      }
    },
    "enrichment": {
      "frequency": "daily",
      "time": "10:00"
    },
    "janitor": {
      "frequency": "weekly",
      "day": "sunday",
      "time": "03:00"
    },
    "scout": {
      "frequency": "hourly"
    },
    "auditor": {
      "frequency": "weekly",
      "day": "monday",
      "time": "09:00"
    },
    "approval": {
      "frequency": "daily",
      "time": "14:00"
    }
  },
  "monitoring": {
    "slack_webhook": null,
    "error_threshold": 5,
    "alert_on_failures": true,
    "metrics": {
      "track_processing_time": true,
      "track_success_rate": true,
      "track_catalog_quality": true
    }
  },
  "firebase": {
    "rate_limits": {
      "requests_per_second": 10,
      "burst_size": 20
    },
    "retry_policy": {
      "max_retries": 3,
      "initial_delay_ms": 1000,
      "max_delay_ms": 10000,
      "exponential_base": 2
    }
  }
}
