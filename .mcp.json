{
  "mcpServers": {
    "firebase": {
      "command": "firebase",
      "args": ["mcp", "--dir", "/Users/valterandersson/Documents/Povver/firebase_functions"],
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "${FIREBASE_SA_KEY}"
      }
    },
    "xcodebuild": {
      "command": "/opt/homebrew/bin/npx",
      "args": ["-y", "xcodebuildmcp@latest", "mcp", "--log-level", "none"]
    },
    "gcloud-observability": {
      "command": "npx",
      "args": ["-y", "@google-cloud/observability-mcp@latest"],
      "env": {
        "GOOGLE_APPLICATION_CREDENTIALS": "${GCP_SA_KEY}",
        "GOOGLE_CLOUD_PROJECT": "myon-53d85"
      }
    }
  }
}
